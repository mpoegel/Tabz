{
  "name": "Tabz",
	"manifest_version": 2,
	"version": "1.0",
  "name": "TabZ",
  "description": "This extension is to make tabs cooler on google chrome, so  Doge",
  "version": "1.0",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Tabz",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"history",
		"tabCapture",
		"topSites"
	],
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"windows": "Ctrl+Shift+K",
				"mac": "Command+Shift+Y",
				"chromeos": "Ctrl+Shift+U",
				"linux": "Ctrl+Shift+J"
			}
		}
	}
}
